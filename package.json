{
  "name": "subcollider",
  "description": "JavaScript utility library that provides like SuperCollider functions",
  "version": "0.2.0",
  "author": "nao yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/subcollider/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^4.6.6",
    "browserify": "^9.0.3",
    "espower-babel": "^1.3.0",
    "isparta": "^2.2.0",
    "jshint": "^2.6.3",
    "jshint-stylish": "^1.0.1",
    "mocha": "^2.1.0",
    "power-assert": "^0.10.2",
    "uglify-js": "^2.4.16"
  },
  "files": [
    "package.json",
    "README.md",
    "lib"
  ],
  "homepage": "https://github.com/mohayonao/subcollider/",
  "keywords": [
    "util",
    "functional",
    "music",
    "supercollider"
  ],
  "licenses": "MIT",
  "main": "./build/subcollider.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohayonao/subcollider.git"
  },
  "scripts": {
    "build": "npm run build-to5 && npm run build-browser && npm run build-minify",
    "build-browser": "browserify lib --standalone sc --outfile build/subcollider.js",
    "build-minify": "uglifyjs build/subcollider.js -o build/subcollider.min.js",
    "build-to5": "babel src --out-dir lib",
    "cover": "babel-node node_modules/.bin/isparta cover --report text --report html node_modules/.bin/_mocha",
    "lint": "jshint src test --reporter node_modules/jshint-stylish/stylish.js",
    "test": "mocha",
    "travis": "npm run lint && npm run test"
  }
}
